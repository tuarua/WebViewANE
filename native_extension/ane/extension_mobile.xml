<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
	<id>com.tuarua.WebViewANE</id>
	<name>WebView ANE</name>
	<copyright>This work is licensed under Apache License, Copyright (c) 2017 Tua Rua Ltd.</copyright>
	<versionNumber>0.0.26</versionNumber>
	<platforms>
        
	<platform name="iPhone-x86">
		<applicationDeployment>
			<nativeLibrary>libWebViewANE.a</nativeLibrary>
			<initializer>TRWVExtInizer</initializer>
			<finalizer>TRWVExtFinizer</finalizer>
		</applicationDeployment>
	</platform>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libWebViewANE.a</nativeLibrary>
            <initializer>TRWVExtInizer</initializer>
            <finalizer>TRWVExtFinizer</finalizer>
        </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>classes.jar</nativeLibrary>
            <initializer>com.tuarua.WebViewANE</initializer>
            <finalizer>com.tuarua.WebViewANE</finalizer>
        </applicationDeployment>
    </platform>
    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>classes.jar</nativeLibrary>
            <initializer>com.tuarua.WebViewANE</initializer>
            <finalizer>com.tuarua.WebViewANE</finalizer>
        </applicationDeployment>
    </platform>
    
     <platform name="default">
        <applicationDeployment/>
    </platform>

	</platforms>
</extension>
